IsLoggedIn(true);\r\n            localStorage.setItem(\"token\", res.data.access_token);\r\n            localStorage.setItem(\"name\", formValues.username);\r\n            push(\"/products\");\r\n            console.log(localStorage.getItem(\"name\"));\r\n            console.log(localStorage.getItem(\"token\"));\r\n          })\r\n          .catch((err) => console.log({err}))\r\n  }\r\n\r\n  const change = (evt) => {\r\n        const { name, value } = evt.target;\r\n        update(name, value);\r\n  };\r\n    \r\n  return (\r\n    <div className='home-wrapper'>\r\n        <div className='item-wrapper'>\r\n      <form className=\"form\" onSubmit={onSubmit}>\r\n        <div>\r\n          <label className='form-row'> \r\n            Username:\r\n            <input\r\n              value={formValues.username}\r\n              onChange={change}\r\n              name=\"username\"\r\n              type=\"text\"\r\n            />\r\n            {formErrors.username}\r\n          </label >\r\n        </div>\r\n        <div>\r\n          <label  className='form-row'>\r\n            Password:\r\n            <input\r\n              value={formValues.password}\r\n              onChange={change}\r\n              name=\"password\"\r\n              type=\"password\"\r\n            />\r\n            {formErrors.password}\r\n          </label>\r\n        </div>\r\n          <div className='buttons'>\r\n            <div className='form-button'>\r\n          <button id=\"submitBtn\"\r\n          disabled={disabled}>Log In</button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n    </div>\r\n  )\r\n\r\n}","C:\\Users\\Admin2\\OneDrive\\Lambda School\\Web_3.3\\BuildWeek2-ContextAPI\\client\\src\\hooks\\useCart.js",[],"C:\\Users\\Admin2\\OneDrive\\Lambda School\\Web_3.3\\BuildWeek2-ContextAPI\\client\\src\\contexts\\ProductContext.js",[],"C:\\Users\\Admin2\\OneDrive\\Lambda School\\Web_3.3\\BuildWeek2-ContextAPI\\client\\src\\components\\Products.js",[],"C:\\Users\